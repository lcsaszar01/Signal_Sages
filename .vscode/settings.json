{
    "files.watcherExclude": {
        "**/target": true
    },
    "[rocket-mvbasic]": {
        "editor.codeLens": false
    },
    "files.associations": {
        "**/.rmv/config/*.json": "jsonc"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.ruby-lsp": true,
        "**/.rmv/catalog/**": true,
        "*/_*": false
    }
}